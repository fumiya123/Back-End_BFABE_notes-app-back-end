{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style":"off",
        "indent":"off",
        "no-console":"off",
        "no-trailing-spaces": "off",
        "no-unused-vars": "off",
        "no-underscore-dangle": "off"
    }
}
